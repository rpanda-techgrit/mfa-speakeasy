{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "brace-style": ["error", "1tbs"],
    "comma-dangle": ["error", "never"],
    "no-unused-vars": ["error"],
    "no-var": ["off"],
    "one-var": ["off"],
    "linebreak-style": 0,
    "no-console": "off",
    "import/extensions": ["error", "ignorePackages"],
    "no-plusplus": "off"
  }
}
